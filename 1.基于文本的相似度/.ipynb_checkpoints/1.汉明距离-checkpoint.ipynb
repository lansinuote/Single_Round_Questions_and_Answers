   "source": [
    "def hamming_distance(s1, s2):\n",
    "    dist = sum(c1 != c2 for c1, c2 in zip(s1, s2))\n",
    "\n",
    "    dist += abs(len(s1) - len(s2))\n",
    "\n",
    "    return dist\n",
    "\n",
    "\n",
    "hamming_distance('aaa', 'abbcc')"
   ]
  }
