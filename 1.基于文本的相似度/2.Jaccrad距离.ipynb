   "source": [
    "def Jaccrad(s1, s2):\n",
    "    #分词\n",
    "    s1 = s1.split(' ')\n",
    "    s2 = s2.split(' ')\n",
    "\n",
    "    #去重\n",
    "    s2 = set(s2)\n",
    "    s1 = set(s1)\n",
    "\n",
    "    #并集数量\n",
    "    bing = len(s1 & s2)\n",
    "\n",
    "    #交集数量\n",
    "    jiao = len(s1 | s2)\n",
    "\n",
    "    #Jaccard距离\n",
    "    return bing / jiao\n",
    "\n",
    "\n",
    "Jaccrad('aa bb cc dd', 'aa bb cc ee')"
   ]
  }
 ],
